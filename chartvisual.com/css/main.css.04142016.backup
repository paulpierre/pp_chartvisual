.candle-stick-label {
    font-size: 11px;
}

.csi-label {
    font-size: 5px;
}

.annotation-y-div {
    position: absolute;
    background-color: rgba(255,255,255,1);
    border: 1.5px solid #bbb;
    z-index: 1;
    width: 62px;
    left: 0;
    top: 0;
    padding: 3px;
    padding-left: 13px;
    color: #666;
    font-weight: 500;
    font-size: 11px;
    display: none;
    white-space: nowrap;
}

.tool-tip-div {
    position: absolute;
    background-color: rgba(255,255,255,0.4);
    border: 1px solid #eee;
    z-index: 1;
    left: 0;
    top: 0;
    padding: 10px;
    display: none;
    white-space: nowrap;

    -webkit-transition: 0.2s ease-in-out;
    -moz-transition: 0.2s ease-in-out;
    -o-transition: 0.2s ease-in-out;
    transition: 0.2s ease-in-out;
}

.guideline-visibility {
    opacity: 1;
}

.price-visibility {
    opacity: 0.4;
}

.laser-visibility {
    opacity: 1;
}

.strength-visibility {
    opacity: 1;
}

